{
    "name": "proposal-page/sdk-php",
    "description": "This module is a open source project with the goal of making the task of consuming the Proposal Page API easy and straight-forward",
    "keywords": [
        "proposal",
        "page",
        "sdk"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gianluca Bine",
            "email": "gian_bine@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "appstract/lush-http": "^0.5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "helmich/phpunit-json-assert": "^3.0",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "ProposalPage\\Sdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProposalPage\\Sdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {}
}
